    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Program",
            "program": "${workspaceFolder}/node_modules/serverless/bin/serverless",
            "request": "launch",
            "args": [
                "offline",
                "start",
                "--httpPort",
                "4000",
                "--noTimeout"
            ],
            "protocol": "inspector",
            "runtimeExecutable": "py",
            "env": {}
        }
    ]
}